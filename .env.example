# Haven Institute Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# FRONTEND VARIABLES (for Vite build)
# ===========================================

# API Configuration
VITE_API_URL=https://api.havenstudy.com/api/v1
VITE_WS_URL=wss://api.havenstudy.com
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_key_here
VITE_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
VITE_APP_ENV=production

# ===========================================
# DOCKER / DEPLOYMENT VARIABLES
# ===========================================

# Domain
DOMAIN=havenstudy.com

# Database
DB_NAME=haven_institute
DB_USER=haven_user
DB_PASSWORD=your_secure_password
DB_ROOT_PASSWORD=your_root_password

# Redis
REDIS_PASSWORD=your_redis_password

# Stripe (production)
STRIPE_SECRET_KEY=sk_live_your_key_here
STRIPE_PUBLISHABLE_KEY=pk_live_your_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_PRO_MONTHLY_PRICE_ID=price_xxx
STRIPE_PRO_YEARLY_PRICE_ID=price_xxx
STRIPE_PREMIUM_MONTHLY_PRICE_ID=price_xxx
STRIPE_PREMIUM_YEARLY_PRICE_ID=price_xxx
